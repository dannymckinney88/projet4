cart.line_items.map(lineItem => (\n              <CartItem\n                item={lineItem}\n                key={lineItem.id}\n                className=\"cart__inner\"\n                handleUpdateCartQty= {handleUpdateCartQty}\n                handleRemoveFromCart= {handleRemoveFromCart}\n              />\n            )):\"\" }\n            <div className=\"col-sm-8\">\n              <p className=\"cart__total-title\">Subtotal:</p>\n              <p className=\"cart__total-price\">{cart ? cart.cart.subtotal.formatted_with_symbol : \"$0.00\"}</p>\n            </div>\n          </>\n        );\n  }\n\n\n    return (\n        \n            <div className=\"container-fluid\">\n            <div className=\"row\">\n                <div className=\"col\">\n                    { renderCart() }\n                    { renderEmptyCart() }\n                </div>\n                <div className=\"col\">\n                    <p className=\"cart__total-title\">Subtotal:</p>\n                    <p className=\"cart__total-price\">{cart ? cart.cart.subtotal.formatted_with_symbol : \"\"}</p>\n                    {cart ?  <Link to={`/checkout/${cart.cart.id}`}>  Checkout </Link> : \"\"}\n                </div>\n            </div>\n\n            </div>\n\n        \n    );\n}\n\nexport default Cart;\n","/Users/user/Documents/SEIR/unit4/project4/src/pages/Home.js",[],"/Users/user/Documents/SEIR/unit4/project4/src/pages/SingleProduct.js",[],"/Users/user/Documents/SEIR/unit4/project4/src/components/Hero.js",[],"/Users/user/Documents/SEIR/unit4/project4/src/components/Products.js",[],"/Users/user/Documents/SEIR/unit4/project4/src/components/ProductItem.js",[],"/Users/user/Documents/SEIR/unit4/project4/src/components/CartItem.js",[],"/Users/user/Documents/SEIR/unit4/project4/src/pages/Checkout.js",["77","78"],"/Users/user/Documents/SEIR/unit4/project4/src/context/Cart.js",[],"/Users/user/Documents/SEIR/unit4/project4/src/components/CheckoutForm.js",["79","80","81","82"],{"ruleId":"83","r